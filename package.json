{
  "name": "scribemd-pro",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Powered Medical Scribe SaaS Platform",
  "packageManager": "npm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:admin": "turbo run dev --filter=admin",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "build:ios": "cd apps/web && npm run build && npx cap sync ios && npx cap open ios",
    "build:android": "cd apps/web && npm run build && npx cap sync android && npx cap open android",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "cap:sync": "cd apps/web && npx cap sync",
    "cap:open:ios": "cd apps/web && npx cap open ios",
    "cap:open:android": "cd apps/web && npx cap open android"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@deepgram/sdk": "^4.11.2",
    "@types/node": "^20.10.0",
    "@vercel/node": "^5.5.5",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
